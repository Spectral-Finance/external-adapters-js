{
  "$id": "https://external-adapters.chainlinklabs.com/schemas/spectral-macro-score-adapter.json",
  "title": "@chainlink/spectral-macro-score-adapter env var schema",
  "required": ["API_KEY", "RPC_URL", "NFC_REGISTRY_ADDRESS"],
  "type": "object",
  "properties": {
